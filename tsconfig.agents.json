{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "outDir": "./api/agents",
    "rootDir": "./api/agents",
    "declaration": false,
    "declarationMap": false,
    "sourceMap": true,
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": [
    "api/agents/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
