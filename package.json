{
  "name": "fortheweebs",
  "version": "2.1.0",
  "description": "Sovereign creator-first platform with full AI production suite, creator economy, and education",
  "main": "electron-main.js",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "npm run fix:react && node scripts/inject-env.js && vite build && npm run check:react",
    "check:react": "node scripts/check-react.js",
    "fix:react": "node scripts/fix-react.js",
    "build:agents": "tsc --project tsconfig.agents.json",
    "preview": "vite preview",
    "server": "node server.js",
    "setup": "echo 'Skipping face models for now'",
    "postinstall": "echo 'Skipping postinstall'",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "check:duplicates": "jscpd --config .jscpd.json .",
    "check:duplicates:ci": "jscpd --config .jscpd.json . --reporters json,console",
    "test": "vitest",
    "analyze": "node scripts/analyze-bundle.js",
    "electron": "npm run build && electron .",
    "electron:build": "npm run build && electron-builder --win --x64",
    "android:build": "npm run build && npx cap sync android && npx cap copy android",
    "android:open": "npx cap open android",
    "android:run": "npm run android:build && npx cap run android",
    "android:release": "npm run build && npx cap sync android && cd android && .\\gradlew assembleRelease && cd ..",
    "android:bundle": "npm run build && npx cap sync android && cd android && .\\gradlew bundleRelease && cd .."
  },
  "keywords": [
    "creator",
    "platform",
    "vanguard",
    "fortheweebs"
  ],
  "author": "Jacob Morris",
  "resolutions": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "**/react": "18.3.1",
    "**/react-dom": "18.3.1"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/fabric": "^5.3.10",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.10.1",
    "@types/pdfkit": "^0.17.4",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^5.1.0",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "esbuild": "0.25.12",
    "eslint": "^8.56.0",
    "fabric": "^6.9.0",
    "file-saver": "^2.0.5",
    "helmet": "^8.1.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "pdfkit": "^0.17.2",
    "terser": "^5.44.1",
    "tone": "^15.1.22",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@aws-sdk/client-s3": "^3.947.0",
    "@aws-sdk/client-ses": "^3.947.0",
    "@aws-sdk/s3-request-presigner": "^3.947.0",
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@ffmpeg/ffmpeg": "^0.12.15",
