    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-windows": "^1.0.0-beta.8",
    "nodemon": "^3.1.9",
    "openai": "^4.79.1",
    "pngjs": "^7.0.0",
    "psd.js": "^3.9.2",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.3.0",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "replicate": "^1.4.0",
    "sharp": "^0.34.5",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.1",
    "three": "^0.181.2",
    "ws": "^8.18.3"
  },
  "build": {
    "appId": "com.fortheweebs.app",
    "productName": "ForTheWeebs",
    "directories": {
      "output": "electron-dist"
    },
    "files": [
      "dist/**/*",
      "electron-main.js",
      "electron-preload.js",
      "server.js",
      "api/**/*",
      "utils/**/*",
      "config/**/*",
      "lib/**/*",
      "node_modules/**/*",
      ".env"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon-512.png",
      "signAndEditExecutable": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
